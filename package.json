{
  "name": "semasim-gateway",
  "description": "Semasim server for gateways (raspberry pi)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.9.0",
  "scripts": {
    "postinstall": "npm run remove-bold-color",
    "remove-bold-color": "sed -i '/bold: string;/d' ./node_modules/@types/colors/index.d.ts",
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "start": "sudo systemctl stop semasim-gateway && DEBUG=_* node ./node_modules/nodemon/bin/nodemon ./dist/lib/main.js",
    "test": "node ./dist/test/db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/semasim-gateway.git"
  },
  "author": "Joseph Garrone",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "async-lock": "^1.0.0",
    "chan-dongle-extended-client": "git+https://a30f7432b117d8320cc21cc03513817c3f08d4b6:x-oauth-basic@github.com/garronej/chan-dongle-extended-client.git",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "debug": "^2.6.8",
    "mysql": "^2.13.0",
    "network": "^0.3.2",
    "rejection-tracker": "github:garronej/rejection-tracker",
    "sip": "github:garronej/sip.js",
    "super-json": "^2.2.0",
    "ts-events-extended": "garronej/ts-events-extended",
    "ws": "^4.0.0",
    "zongji": "^0.4.4"
  },
  "devDependencies": {
    "@types/async-lock": "0.0.20",
    "@types/colors": "^1.1.3",
    "@types/debug": "0.0.29",
    "@types/mysql": "0.0.32",
    "@types/node": "^6.0.96",
    "@types/ws": "^4.0.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.7.1"
  }
}
