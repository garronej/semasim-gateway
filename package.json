{
  "name": "semasim-gateway",
  "description": "Semasim server for gateways (raspberry pi)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.16.4",
  "scripts": {
    "postinstall": "cp $(readlink -e $(which node)) ./node",
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "start": "sudo ./node ./dist/bin/main.js",
    "test": "sudo ./node ./dist/test/main.js",
    "tarball": "sudo node dist/bin/installer tarball",
    "build-sqlite": "(cd ./node_modules/sqlite3/ && ./node_modules/node-pre-gyp/bin/node-pre-gyp install --fallback-to-build)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/semasim-gateway.git"
  },
  "author": "Joseph Garrone",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "chan-dongle-extended-client": "github:garronej/chan-dongle-extended-client",
    "transfer-tools": "garronej/transfer-tools"
  },
  "devDependencies": {
    "@types/async-lock": "0.0.20",
    "@types/node": "^8.9.1",
    "@types/ws": "^4.0.0",
    "typescript": "^2.9.2",
    "async-lock": "^1.0.0",
    "commander": "^2.11.0",
    "ini-extended": "github:garronej/ini-extended",
    "logger": "github:garronej/logger",
    "network": "^0.3.2",
    "sqlite": "^2.9.1",
    "scripting-tools": "github:garronej/scripting-tools",
    "sqlite-custom": "github:garronej/sqlite-custom",
    "ts-ami": "github:garronej/ts-ami",
    "ts-events-extended": "garronej/ts-events-extended",
    "ts-sip": "github:garronej/ts-sip",
    "ws": "^4.0.0"
  }
}
